{
    "manifest_version": 2,
    "name": "Wikipedia_DarkMode",
    "version": "1.1",
  
    "description": "Make a better wikipedia dark mode",

    "content_scripts": [
      {
        "matches": ["*://*.wikipedia.org/*"],
        "js": ["script/wikipedia.js"]
      },
      {
        "matches": ["*://news.ycombinator.com/*"],
        "js": ["script/hackernews.js"]
      }
    ]
    
  }
  